<DataObject xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns="http://sifassociation.org/SpecGen" name="RoomType">
  <Key>@RefId</Key>
  <EventsReported>true</EventsReported>
  <Item>
    <Element>RoomType</Element>
    <Description>
      <p>This object identifies the type of room, i.e. gym, cafeteria, standard classroom, etc.</p>
    </Description>
  </Item>
  <Item>
    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>GUID that identifies this room type.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"/>
  </Item>
  <Item>
    <Attribute>SchoolInfoRefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>GUID that identifies the school that this room type belongs to.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"/>
    <cedsId>000504</cedsId>
    <cedsURL>https://ceds.ed.gov/element/000504</cedsURL>
  </Item>
  <Item>
    <Element>Description</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Description of the room type.</p>
    </Description>
    <Type name="xs:normalizedString"/>
  </Item>
  <Item>
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
    <Type ref="CommonTypes" name="SIF_MetadataType"/>
  </Item>
  <Item>
    <Element>SIF_ExtendedElements</Element>
    <Characteristics>O</Characteristics>
    <Type ref="CommonTypes" name="SIF_ExtendedElementsType"/>
  </Item>
  <xhtml:Example xmlns="">
    <RoomType RefId="D3E34B359D75101A8C3D00AA001A1652" SchoolInfoRefId="A2E35B359D75101A8C3D00AA001A0000">
      <Description>Gymnasium</Description>
    </RoomType>
  </xhtml:Example>
</DataObject>
