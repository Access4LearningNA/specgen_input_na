<DataObject xmlns="http://sifassociation.org/SpecGen" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:json="http://json.org/" name="xStaff">
  <Key>@refId</Key>
  <EventsReported>true</EventsReported>
  <Item>
    <Element>xStaff</Element>
    <Description>
      <p>xStaff is a composite object that provides a view of the most commonly used core data elements for teachers and non-teaching staff. The refId of a xStaff object coincides with the refId of a corresponding staff object.</p>
    </Description>
  </Item>
  <Item>
    <Attribute>refId</Attribute>
    <Characteristics>M</Characteristics>
    <Type ref="CommonTypes" name="gRefIdType"/>
  </Item>
  <Item>
    <Element>name</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Name of the staff person.</p>
    </Description>
    <Type ref="CommonTypes" name="xPersonNameType"/>
  </Item>
  <Item>
    <Element>localId</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>A unique number or alphanumeric code assigned to a staff member by a district or LEA.</p>
    </Description>
    <Type ref="CommonTypes" name="xPersonIdType"/>
  </Item>
  <Item>
    <Element>loginId</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The login identifier for the person.</p>
    </Description>
    <Type ref="CommonTypes" name="xLoginIdType"/>
    <cedsId>001170</cedsId>
    <cedsURL>https://ceds.ed.gov/element/001170</cedsURL>
  </Item>
  <Item>
    <Element>stateProvinceId</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>A unique number or alphanumeric code assigned to a staff member by a state.</p>
    </Description>
    <Type ref="CommonTypes" name="xPersonIdType"/>
    <cedsId>001070</cedsId>
    <cedsURL>https://ceds.ed.gov/element/001070</cedsURL>
  </Item>
  <Item>
    <Element>otherIds</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Other Ids for the person.</p>
    </Description>
    <Type ref="CommonTypes" name="xOtherPersonIdListType"/>
  </Item>
  <Item>
    <Element>sex</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The concept describing the biological traits that distinguish the males and females of a species.</p>
    </Description>
    <Type ref="CodeSets" name="xSexType"/>
    <cedsId>000255</cedsId>
    <cedsURL>https://ceds.ed.gov/element/000255</cedsURL>
  </Item>
  <Item>
    <Element>email</Element>
    <Characteristics>O</Characteristics>
    <Type ref="CommonTypes" name="xEmailType"/>
    <cedsId>000088</cedsId>
    <cedsURL>https://ceds.ed.gov/element/000088</cedsURL>
  </Item>
  <Item>
    <Element>primaryAssignment</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The primary district, school, and function to which the person is assigned.</p>
    </Description>
    <Type ref="CommonTypes" name="xStaffPersonAssignmentType"/>
  </Item>
  <Item>
    <Element>otherAssignments</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Other locations and functions to which the person is assigned.</p>
    </Description>
    <Type ref="CommonTypes" name="xStaffPersonAssignmentListType"/>
  </Item>
  <xhtml:Example xmlns="">
    <xStaff refId="0A363E64-8A65-4873-A971-006192DE8D55">
      <name>
        <type>LegalName</type>
        <familyName>Olson</familyName>
        <givenName>Brandon</givenName>
        <middleName>X</middleName>
      </name>
      <otherIds>
        <otherId>
          <type>School</type>
          <id>1358627</id>
        </otherId>
        <otherId>
          <type>District</type>
          <id>471442674</id>
        </otherId>
        <otherId>
          <type>PIN</type>
          <id>6348</id>
        </otherId>
        <otherId>
          <type>State</type>
          <id>847585242</id>
        </otherId>
      </otherIds>
      <sex>Male</sex>
      <email>
        <emailType>Organizational</emailType>
        <emailAddress>BrandonOlson@JohnsonCityMS.edu</emailAddress>
      </email>
      <primaryAssignment>
        <leaRefId>9BA8379D-9A9D-4D59-9202-E976CADF5526</leaRefId>
        <schoolRefId>66667705-6C51-4C30-A22A-77CEA0FBCF53</schoolRefId>
        <jobFunction>Secondary Teachers</jobFunction>
      </primaryAssignment>
    </xStaff>
  </xhtml:Example>
</DataObject>
